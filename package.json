    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/**/*.js",
      "!**/coverage/**",
      "!**/bin/**",
      "!**/node_modules/**",
      "!**/public/**",
      "!**/tests/**",
      "!**/webpack/**",
      "!**/webpack.config.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "transform": {
      ".*.js": "<rootDir>/node_modules/babel-jest"
